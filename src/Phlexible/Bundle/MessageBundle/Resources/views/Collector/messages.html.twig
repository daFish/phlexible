{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block menu %}
    <span class="label">
        <span class="icon"><img
                    src="data:image/png;base64,"
                    alt="Messages"></span>
        <strong>Messages</strong>
    </span>
{% endblock %}

{% block panel %}

    Types:<br/>{% for type in collector.types %}- {{ type.name }}: {{ type.count }}<br/>{% endfor %}<br/>
    Errors: {{ collector.countErrors }}<br/>
    <br/>
    {% for message in collector.messages %}
        {{ message.createdAt }}
        {{ message.subject }}
        [{{ message.typeName }}]
        [{{ message.channel }}]
        [{{ message.role }}]
        by {{ message.user }}
        <br/>
        {% if message.body %}
            {{ message.body }}
            <br/>
        {% endif %}
    {% endfor %}

{% endblock %}
